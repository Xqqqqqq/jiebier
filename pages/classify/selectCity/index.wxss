page{
  height: 100%;
  width: 100%;
}
.wrap{
  width: 100%;
  height: 100%;
  padding: 24rpx;
  box-sizing: border-box;
}
.wrap-pos{
  font-size: 30rpx;
  color: #010101;
  font-weight: bold;
  margin-bottom: 55rpx;
}
.wrap-pos text{
  font-size: 30rpx;
  color: #1579DF;
  margin-left: 26rpx;
}
.wrap-recommend{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.recommend-li-ture{
  padding: 0 15rpx;
  box-sizing: border-box;
  text-align: center;
  height:70rpx;
  line-height: 70rpx;
  border:1px solid rgba(204,204,204,1);
  border-radius:31rpx;
  font-size: 30rpx;
  color: #333;
  box-sizing: border-box;
  margin-right: 25rpx;
  margin-bottom: 25rpx;
  min-width: 145rpx;
  background-color: #F7F7F7;
}
.recommend-li{
  padding: 0 15rpx;
  box-sizing: border-box;
  text-align: center;
  height:70rpx;
  line-height: 70rpx;
  border:1px solid rgba(204,204,204,1);
  border-radius:31rpx;
  font-size: 30rpx;
  color: #333;
  box-sizing: border-box;
  margin-right: 25rpx;
  margin-bottom: 25rpx;
  min-width: 145rpx;
}
.i-index-demo-item{
  height: 98rpx;
  line-height: 98rpx;
  border-bottom: 1px solid #D4D4D4;
}