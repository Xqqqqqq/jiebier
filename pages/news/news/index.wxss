.wrap{
  width: 100%;
  padding: 24rpx;
  box-sizing: border-box;
}
.wrap-message{
  width: 100%;
  margin-bottom: 34rpx;
}
.message-top{
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 34rpx;
}
.message-top-left{
  font-size: 30rpx;
  color: #000000;
}
.message-top-right{
  font-size: 24rpx;
  color: #565656;
}
.message-bottom{
  width: 100%;
  padding: 33rpx;
  box-sizing: border-box;
  box-shadow:0px 2px 8px 0px #EFEFEF;
}
.message-bottom-title{
  margin-bottom: 27rpx;
  font-size: 30rpx;
  color: #000000;
}
.message-bottom-tip{
  font-size: 24rpx;
  color: #565656;
}