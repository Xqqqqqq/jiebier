page{
  background-color: #F8F8F8;
}
.wrap{
  width: 100%;
}
.wrap-top{
  width: 100%;
  padding: 21rpx 25rpx;
  box-sizing: border-box;
  background: linear-gradient(90deg, #FF774D, #FF0149);
  margin-bottom: 32rpx;
}
.wrap-top-img{
  width: 100%;
  height: 700rpx;
  background-color: #fff;
}
.wrap-top-img image{
  width: 100%;
  height: 100%;
}
.wrap-top-detail{
  width: 100%;
  height: 136rpx;
  padding-top: 35rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  color: #fff;
}
.top-detail-left{
  font-size: 24rpx;
  margin-top: 1%;
}
.detail-left-pirce{
  font-size: 48rpx;
  font-weight: bold;
  margin-right: 10rpx;
}
.detail-left-origin{
  text-decoration: line-through;
}
.detail-right-top{
  font-size: 40rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
  text-align: right;
}
.detail-right-bottom{
  font-size: 24rpx;
  display: flex;
}
.detail-right-bottom text{
  margin-right: 10rpx;
}
.wrap-box{
  width: 100%;
  padding: 0 25rpx;
  box-sizing: border-box;
}
.wrap-box-ul{
  width: 100%;
  background-color: #fff;
  border-radius: 20px;
  padding-top: 30rpx;
  padding-left: 25rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
}
.box-ul-li{
  text-align: center;
  margin-bottom: 30rpx;
  margin-right: 30rpx;
}
.ul-li-img{
  overflow: hidden;
  width: 105rpx;
  height: 105rpx;
  border-radius: 50%;
}
.ul-li-img image{
  width: 100%;
  height: 100%;
}
.ul-li-text{
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #050505;
  margin-top: 13rpx;
}
.wrap-bottom{
  width: 100%;
  display: flex;
  flex-direction: row-reverse;
  padding-right: 25rpx;
  padding-bottom: 46rpx;
  box-sizing: border-box;
  margin-top: 60rpx;
}
.wrap-bottom-btn{
  width: 255rpx;
  height: 84rpx;
  border-radius: 42rpx;
  color: #fff;
  font-weight: bold;
  font-size: 32rpx;
  line-height: 84rpx;
  text-align: center;
}
.bottom-btn-red{
  background: linear-gradient(90deg, #FF774D, #FF0149);
  margin-left: 20rpx;
}
.bottom-btn-yellow{
  background: linear-gradient(90deg, #FBCE44, #FF7900);
  padding: 0;
  margin: 0;
  border: none !important;
}
.wrap-bottom-clone{
  width: 100%;
  height: 130rpx;
}