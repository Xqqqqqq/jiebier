page{
  background-color: #f4f4f4;
}
.wrap{
  width: 100%;
}
.wrap-tab{
  width: 100%;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
}
.tab-li-ture{
  color: green;
  font-size: 34rpx;
  font-weight: bold;
  padding-bottom: 30rpx;
  border-bottom: 4rpx solid green;
  box-sizing: border-box;
}
.tab-li{
  color: #333333;
  font-size: 34rpx;
  font-weight: bold;
  padding-bottom: 30rpx;
  border-bottom: 4rpx solid #fff;
  box-sizing: border-box;
}
/* liåŒº */
.wrap-box{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.wrap-box-li{
  width: 100%;
  border-radius: 20rpx;
  background-color: #fff;
  padding: 40rpx;
  box-sizing: border-box;
  display: flex;
  margin-bottom: 20rpx;
}
.box-li-left{
  width: 200rpx;
  height: 200rpx;
  background-color: red;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.box-li-left image{
  width: 100%;
  height: 100%;
}
.box-li-right{
  width: 65%;
}
.li-right-title{
  font-size: 34rpx;
  font-weight: bold;
  color: #333333;
  margin-bottom: 10rpx;
}
.li-right-name{
  font-size: 28rpx;
  color: #333333;
  margin-bottom: 10rpx;
}
.li-right-num{
  font-size: 28rpx;
  color: #666666;
  margin-bottom: 10rpx;
}
.li-right-price{
  font-size: 36rpx;
  color: orange;
  position: relative;
  width: 100%;
}
.li-right-price text{
  font-size: 24rpx;
}
.li-bottom-price{
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  display: flex;
}
.bottom-price-add{
  width: 40rpx;
  height: 40rpx;
  line-height: 35rpx;
  background-color: green;
  text-align: center;
  border-radius: 50%;
}
.bottom-price-del{
  width: 40rpx;
  height: 40rpx;
  line-height: 30rpx;
  border: 1px solid #333333;
  text-align: center;
  border-radius: 50%;
  font-size: 55rpx;
  color: #333333;
  box-sizing: border-box;
}
.bottom-price-num{
  font-size: 30rpx;
  color: #333333;
  margin: 0 10rpx;
}