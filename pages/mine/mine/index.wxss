.wrap{
  width: 100%;
  padding: 24rpx;
  box-sizing: border-box;
}
.my-box{
  border-radius:10rpx;
  box-shadow:0px 2px 8px 0px #EFEFEF;
  width: 100%;
  padding: 37rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}
.my-box-unlogin{
  padding-top: 50rpx;
  padding-bottom: 50rpx;
}
.my-box-other{
  padding: 0;
  box-sizing: border-box;
}
.unlogin-title{
  font-size: 42rpx;
  color: #000000;
  font-weight: bold;
  margin-bottom: 31rpx;
}
.unlogin-tip{
  font-size: 24rpx;
  color: #999999;
}
.my-box-line{
  border-bottom: 1px solid #E6E6E6;
  width: 95%;
  margin: auto;
}
.my-box-block{
  display: flex;
  width: 100%;
  justify-content: space-between;
  padding: 36rpx;
  box-sizing: border-box;
}
.box-block-li{
  text-align: center;
}
.box-block-img{
  width: 88rpx;
  height: 88rpx;
  background-color: red;
  border-radius:10rpx;
  margin: auto;
  margin-bottom: 18rpx;
}
.box-block-img image{
  width: 100%;
  height: 100%;
} 
.box-block-name{
  font-size: 28rpx;
  color: #202020;
}