.wrap{
  width: 100%;
  padding: 24rpx;
  box-sizing: border-box;
}
.my-box{
  border-radius:10rpx;
  box-shadow:0px 2px 8px 0px #EFEFEF;
  width: 100%;
  padding: 37rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}
.my-box-unlogin{
  padding-top: 50rpx;
  padding-bottom: 50rpx;
  position: relative;
  background: url('https://www.jiebier.cn/files/group1/M00/00/02/rBo_71_AXG6AetG0AAA6vXJj-Pc310.jpg') no-repeat;
  background-size: 100% 100%;
}
.unlogin-img{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 50rpx;
  width: 124rpx;
  height: 124rpx;
  border-radius: 50%;
  overflow: hidden;
}
.unlogin-img image{
  width: 100%;
  height: 100%;
}
.my-box-other{
  padding: 0;
  box-sizing: border-box;
}
.unlogin-title{
  font-size: 42rpx;
  color: #fff;
  font-weight: bold;
  margin-bottom: 31rpx;
}
.unlogin-tip{
  font-size: 24rpx;
  color: #fff;
}
.my-box-line{
  border-bottom: 1rpx solid #f4f4f6;
  width: 95%;
  /* height: 1rpx;
  background-color: #e9eaec; */
  margin: auto;
}
.my-box-block{
  display: flex;
  width: 100%;
  justify-content: space-between;
  padding: 36rpx;
  box-sizing: border-box;
}
.box-block-li{
  text-align: center;
}
.box-block-img{
  width: 88rpx;
  height: 88rpx;
  border-radius:10rpx;
  margin: auto;
  margin-bottom: 18rpx;
}
.box-block-img image{
  width: 100%;
  height: 100%;
} 
.box-block-name{
  font-size: 28rpx;
  color: #202020;
}
button{
  line-height: 1;
  margin:0;
  padding: 0;
  display: inline-block;
  text-align: left;
  background-color: rgba(89, 194, 5, .1);
}
button::after{
  border:none;
}
input{
  outline:none;
  border:none;
  list-style: none;
}
.my-box-other-icon{
  width: 35rpx;
  height: 35rpx;
  margin-right: 5rpx;
}
.my-box-other-icon image{
  width: 100%;
  height: 100%;
}