page{
  width: 100%;
  background-color: #F8F8F8;
}
.wrap{
  width: 100%;
}
.wrap-tab{
  width: 100%;
  padding-top: 36rpx;
  box-sizing: border-box;
}
.tab-li-ture-other{
  padding-bottom: 27rpx;
  font-size: 32rpx;
}
.wrap-box{
  width: 100%;
  padding: 30rpx 25rpx;
  box-sizing: border-box;
}
.wrap-box-li{
  box-shadow:0px 10px 10px 0px rgba(196,196,196,0.1);
  width: 100%;
  padding: 37rpx 26rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
  position: relative;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
}
.box-li-left{
  display: flex;
  margin-top: 12rpx;
}
.li-left-img{
  width:147rpx;
  height:148rpx;
  border-radius:20rpx;
  margin-right: 25rpx;
  /* background-color: red; */
}
.li-left-img image{
  width: 100%;
  height: 100%;
}
.li-left-detail{
  width: 270rpx;
}
.left-detail-name{
  font-size: 32rpx;
  color: #000000;
  font-weight: bold;
  line-height: 40rpx;
  word-break: break-all;
  width: 100%;
  margin-bottom: 10rpx;
}
.left-detail-time{
  font-size: 20rpx;
  color: #565656;
  margin-bottom: 5rpx;
}
.left-detail-tip{
  font-size: 20rpx;
  color: #565656;
}
.box-li-right{
  text-align: center;
}
.li-right-price{
  font-size: 48rpx;
  font-weight: bold;
  color: #F44031;
  margin-bottom: 10rpx;
}
.li-right-price text{
  font-size: 24rpx;
  font-weight: normal;
}
.li-right-tip{
  font-size: 22rpx;
  color: #565656;
  margin-bottom: 15rpx;
}
.li-right-btn{
  width:132rpx;
  height:49rpx;
  line-height: 49rpx;
  color: #fff;
  text-align: center;
  margin: auto;
  font-size: 24rpx;
  background:rgba(238,65,55,1);
  border-radius:25rpx;
}
.wrap-box-pos{
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  position: absolute;
  top:20%;
  right: 20%;
}
.wrap-box-pos image{
  width: 100%;
  height: 100%;
}