.wrap{
  width: 100%;
  padding-top: 47rpx;
  box-sizing: border-box;
}
.tab-li-ture-other{
  padding-bottom: 27rpx;
  font-size: 32rpx;
}
.wrap-box{
  width: 100%;
  padding: 24rpx;
  box-sizing: border-box;
}
.wrap-box-li{
  width: 100%;
  margin-bottom: 29rpx;
  border-radius: 10rpx;
  padding: 24rpx;
  box-sizing: border-box;
  box-shadow:0px 2px 8px 0px #EFEFEF;
}
.box-li-top{
  width: 100%;
  display: flex;
  position: relative;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #eeeeee;
}
.li-top-img{
  width:120rpx;
  height:120rpx;
  border-radius:20rpx;
  margin-right: 24rpx;
  background-color: red;
}
.li-top-img image{
  width:100%;
  height:100%;
} 
.li-top-right{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.top-right-title{
  font-size: 32rpx;
  color: #000000;
}
.top-right-time{
  font-size: 22rpx;
  color: #565656;
}
.li-top-status{
  font-size: 24rpx;
  position: absolute;
  top: 5rpx;
  right: 10rpx;
}
.box-li-content{
  width: 100%;
  margin-top: 30rpx;
}
.li-content-line{
  display: flex;
  justify-content: space-between;
  margin-bottom: 24rpx;
}
.content-line-left{
  font-size: 24rpx;
  color: #565656;
}
.content-line-left text{
  font-size: 24rpx;
  color: #565656;
  margin-left: 18rpx;
}
.content-line-right{
  font-size: 30rpx;
  color: #000000;
}
.content-line-right text{
  font-size: 18rpx;
}
.box-li-btn{
  width:162rpx;
  height:60rpx;
  line-height: 60rpx;
  background:rgba(255,255,255,1);
  border:1px solid rgba(55,55,55,1);
  border-radius:30rpx;
  font-size: 28rpx;
  color: #373737;
  text-align: center;
  margin-left: auto;
}
.box-li-btn-orange{
  border: 1px solid #EC5E5B;
  color: #EC5E5B;
}