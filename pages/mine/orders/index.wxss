page{
  background-color: #F8F8F8;
}
.wrap{
  width: 100%;
  /* padding-top: 47rpx;
  box-sizing: border-box; */
}
.wrap-top{
  width: 100%;
  height: 47rpx;
  background-color: #fff;
}
.tab-li-ture-other{
  padding-bottom: 27rpx;
  font-size: 32rpx;
}
.wrap-box{
  width: 100%;
  padding: 24rpx;
  box-sizing: border-box;
}
.wrap-box-li{
  width: 100%;
  margin-bottom: 29rpx;
  border-radius: 10rpx;
  padding: 24rpx;
  box-sizing: border-box;
  box-shadow:0px 2px 8px 0px #EFEFEF;
  background-color: #fff;
}
.box-li-top{
  width: 100%;
  display: flex;
  position: relative;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f4f4f6;
}
.li-top-img{
  width:100rpx;
  height:100rpx;
  border-radius:20rpx;
  margin-right: 24rpx;
  overflow: hidden;
  border-bottom: 1rpx solid #f4f4f6;
}
.li-top-img image{
  width:100%;
  height:100%;
} 
.li-top-right{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 100%;
}
.top-right-title{
  font-size: 32rpx;
  color: #000000;
  font-weight: bold;
  margin-bottom: 20rpx;
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.top-right-time{
  font-size: 24rpx;
  color: #565656;
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.li-top-status{
  font-size: 32rpx;
  position: absolute;
  top: 5rpx;
  right: 0rpx;
}
.box-li-content{
  width: 100%;
  margin-top: 30rpx;
}
.li-content-line{
  display: flex;
  /* justify-content: space-between; */
  margin-bottom: 24rpx;
}
.content-line-left{
  font-size: 28rpx;
  color: #565656;
  display: flex;
  justify-content: space-between;
}
.content-line-left text{
  font-size: 28rpx;
  color: #565656;
  margin-left: 18rpx;
}
.content-line-right{
  font-size: 30rpx;
  color: #000000;
}
.content-line-right text{
  font-size: 18rpx;
}
.box-li-btn{
  width:162rpx;
  height:60rpx;
  line-height: 60rpx;
  background:rgba(255,255,255,1);
  border:1px solid rgba(55,55,55,1);
  border-radius:30rpx;
  font-size: 28rpx;
  color: #373737;
  text-align: center;
  margin-left: auto;
}
.box-li-btn-green{
  border: 1px solid#4FBE00;
  color:#4FBE00;
}
.content-line-box{
  margin-left: 15rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 75%;
}