page{
  background-color: #D75A50;
  width: 100%;
}
.wrap-code{
  position: relative;
  width: 100%;
  height: 100%;
}
.wrap-code-img{
  width: 450rpx;
  height: 450rpx;
  position: fixed;
  left: 50%;
  top: 40%;
  /* border-radius: 50%; */
  transform: translate(-50%, -50%);
  overflow: hidden;
}
.wrap-code-img image{
  width: 100%;
  height: 100%;
  display: block;
}