page{
  background-color: #ffffff;
}
.wrap{
  width: 100%;
  padding: 24rpx;
  box-sizing: border-box;
}
.wrap-bg{
  width: 100%;
  height: 270rpx;
  background-color: red;
  margin-bottom: 61rpx;
  border-radius: 10rpx;
  overflow: hidden;
  text-align: center;
  padding-top: 70rpx;
  box-sizing: border-box;
}
.wrap-bg-title{
  font-size: 65rpx;
  font-weight: bold;
  color: #FFFFFF;
  margin-bottom: 25rpx;
}
.wrap-bg-name{
  font-size: 24rpx;
  color: #FFFFFF;
}
.wrap-rule-title{
  font-size: 36rpx;
  font-weight: bold;
  color: #000000;
  margin-bottom: 49rpx;
}
.wrap-content{
  font-size: 24rpx;
  color: #333333;
  line-height: 48rpx;
}
